    conductor -> clients [ arrowhead="box" style="solid" ];
    conductor -> conductor_proxy [ arrowhead="box" style="solid" ];
    conductor -> counters_manager [ arrowhead="box" style="solid" ];
    conductor -> error_log [ arrowhead="box" style="solid" ];
